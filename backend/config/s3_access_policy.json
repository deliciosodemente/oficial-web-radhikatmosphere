{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "NvidiaModelAccess",
            "Effect": "Allow",
            "Principal": {
                "AWS": "*"
            },
            "Action": [
                "s3:GetObject",
                "s3:PutObject",
                "s3:ListBucket"
            ],
            "Resource": [
                "arn:aws:s3:::radhikatmosphere--use1-az6--x-s3",
                "arn:aws:s3:::radhikatmosphere--use1-az6--x-s3/.nvidia-omniverse/*"
            ],
            "Condition": {
                "StringLike": {
                    "aws:SourceArn": "arn:aws:*:*:*:*"
                }
            }
        }
    ]
}